<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/likes_image_list_more_btn_container"
        android:layout_width="@dimen/small_image_size"
        android:layout_height="@dimen/small_image_size"
        android:background="@drawable/circular_view">

        <ImageView
            android:id="@+id/likes_image_list_more_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="5dp"
            android:scaleType="centerInside"
            android:src="@drawable/ic_more_dark" />
    </FrameLayout>

</FrameLayout>